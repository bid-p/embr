<library>
    <repositories>
        <repository>
            <path>{{ modm_path }}</path>
        </repository>
    </repositories>

    <options>
        <option name="modm:build:project.name">embr</option>
        <option name="modm:build:scons:include_sconstruct">False</option>
{% for o in metadata_options %}{{ o }}{% endfor %}
    </options>

    <modules>
{% for m in metadata_modules %}{{ m }}{% endfor %}
    </modules>
</library>